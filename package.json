{
  "name": "monorepo-template-base",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint -- --fix",
    "lint:ci": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:ci": "prettier --check \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo check-types",
    "test": "turbo test"
  },
  "devDependencies": {
    "@yoshinani/style-guide": "^0.14.3",
    "eslint": "^9.38.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.3",
    "vitest": "^4.0.4"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=22"
  },
  "prettier": "@yoshinani/style-guide/prettier"
}
